	    if(GetPVarInt(playerid,"idtaxi")!=0)
	    {
		    if(GetPVarInt(playerid,"idtaxi")!=GetPlayerVehicleID(playerid)) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля");
			DeletePVar(playerid,"deltaxi2");
		}
	    if(GetPVarInt(playerid,"taxi")==1 && GetPVarInt(playerid,"idtaxi")==0) return SPD(playerid,dJob+9,list,"" #C_GRAY "Выберите вид тарифа на перевозку","" #C_GRAY "1. {ffffff}Такса по времени\n" #C_GRAY "2. {ffffff}Такса по расстоянию\n" #C_GRAY "3. {ffffff}Свободная такса","Ок","Закрыть");
	}*/

	if(newcar >= truck[0] && newcar <= truck[1] && GetPlayerVehicleSeat(playerid)==0)
	{
		if(GetPVarInt(playerid,"truck")!=1) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этой фуры");
	    if(GetPVarInt(playerid,"idtruck")!=0)
	    {
	    	if(GetPVarInt(playerid,"idtruck")!=GetPlayerVehicleID(playerid)) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этой фуры");
			DeletePVar(playerid,"deltruck");
		}
	    if(GetPVarInt(playerid,"truck")==1 && GetPVarInt(playerid,"idtruck")==0) return SetPlayerCheckpoint(playerid,751.1165,2232.1245,12.3600,7.0), SetPVarInt(playerid,"idtruck",GetPlayerVehicleID(playerid)), SCM(playerid,-1,"Отправляйтесь на загрузку");
	}
	else if(newcar >= fsbcar[0] && newcar <= fsbcar[1] && GetPlayerVehicleSeat(playerid)==0)
	{
		if(pInfo[playerid][pFrak] != 8)
		{
			SendClientMessage(playerid,-1,"У вас нету ключей");
			return RemovePlayerFromVehicle(playerid);
		}
	}
	else if(stadia[playerid] != 0)
	{
		return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Вы находитесь в стадии гибели!");
	}
	else if(newcar >= omon[0] && newcar <= omon[1] && GetPlayerVehicleSeat(playerid)==0 ) { if(pInfo[playerid][pFrak]!=2) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля ФСВНГ"); }
	else if(newcar >= gibdd[0] && newcar <= gibdd[1] && GetPlayerVehicleSeat(playerid) == 0)
	{
		if(pInfo[playerid][pFrak] != 3) {
		    SendClientMessage(playerid, -1, "Вы не состоите в организации ОГИБДД");
		    SCM(playerid, -1, "У вас нет ключа от этого автомобиля ОГИБДД");
		    return RemovePlayerFromVehicle(playerid);
		}
		else if(GetVehicleModel(newcar) == 525) {
		    SCM(playerid, -1, "Используйте команду " #C_GRAY "/tow, {ffffff}чтобы прицепить транспорт");
		}
	}

	else if(((newcar >= mcs[0] && newcar <= mcs[1] ) || newcar == msc ) && GetPlayerVehicleSeat(playerid)==0) { if(pInfo[playerid][pFrak]!=5) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля МЧС"); }
	else if(newcar >= fkc[0] && newcar <= fkc[1] && GetPlayerVehicleSeat(playerid)==0) { if(pInfo[playerid][pFrak]!=6) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля ФКС"); }
	else if(newcar >= trk[0] && newcar <= trk[1] && GetPlayerVehicleSeat(playerid)==0) { if(pInfo[playerid][pFrak]!=7) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля ТРК"); }
	else if(newcar >= ymvd[0] && newcar <= ymvd[1] && GetPlayerVehicleSeat(playerid)==0) { if(pInfo[playerid][pFrak]!=1) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля ОМВД"); }

	else if(GetPlayerVehicleID(playerid) >= acar_b[0] && GetPlayerVehicleID(playerid) <= acar_b[1]) // Легковушки
	{
		if(GetPVarInt(playerid,"go")==2 && GetPVarInt(playerid,"ekzamen")==1 ) SetPlayerRaceCheckpoint(playerid,0,B_CHECK1,B_CHECK2,7.0), SetPVarInt(playerid,"practicexam",1), SetPVarInt(playerid,"LESSON",1);
    	else return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Вы не сдаете на права или это не ваша категория");
	}

	else if(GetPlayerVehicleID(playerid) >= acar_a[0] && GetPlayerVehicleID(playerid) <= acar_a[1]) // Мотоциклы
	{
		if(GetPVarInt(playerid,"go")==1 && GetPVarInt(playerid,"ekzamen")==1 ) SetPlayerRaceCheckpoint(playerid,0,B_CHECK1,B_CHECK2,7.0), SetPVarInt(playerid,"practicexam",1), SetPVarInt(playerid,"LESSON",1);
    	else return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Вы не сдаете на права или это не ваша категория");
	}

	else if(GetPlayerVehicleID(playerid) >= acar_c[0] && GetPlayerVehicleID(playerid) <= acar_c[1]) // Камазы
	{
		if(GetPVarInt(playerid,"go")==3 && GetPVarInt(playerid,"ekzamen")==1 ) SetPlayerRaceCheckpoint(playerid,0,B_CHECK1,B_CHECK2,7.0), SetPVarInt(playerid,"practicexam",1), SetPVarInt(playerid,"LESSON",1);
    	else return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Вы не сдаете на права или это не ваша категория");
	}

	else if(GetPlayerVehicleID(playerid) >= acar_d[0] && GetPlayerVehicleID(playerid) <= acar_d[1]) // Автобусы
	{
		if(GetPVarInt(playerid,"go")==4 && GetPVarInt(playerid,"ekzamen")==1 ) SetPlayerRaceCheckpoint(playerid,0,B_CHECK1,B_CHECK2,7.0), SetPVarInt(playerid,"practicexam",1), SetPVarInt(playerid,"LESSON",1);
    	else return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Вы не сдаете на права или это не ваша категория");
	}

	// СПИДОМЕТР
	if(newstate == PLAYER_STATE_DRIVER)
    {
        for(new z; z != LoadAllCar; z++)
		{
			if(!strcmp(cInfo[z][cOwner],pInfo[playerid][pName],true))
			{
		    	if(cInfo[z][cCar] == newcar)
		    	{
		    	    if(pInfo[playerid][pVip] == 0 && (cInfo[z][cModel] == 560 ||cInfo[z][cModel] == 580 || cInfo[z][cModel] == 400 || cInfo[z][cModel] == 466 || cInfo[z][cModel] == 579 || cInfo[z][cModel] == 604)) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Для того, чтобы сесть в данный автомобиль необходим ВИП аккаунт!");
		    	    else if(pInfo[playerid][pVip] == 1 && (cInfo[z][cModel] == 580 || cInfo[z][cModel] == 400 || cInfo[z][cModel] == 466 || cInfo[z][cModel] == 579 || cInfo[z][cModel] == 604)) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Чтобы сесть в данный транспорт необходим более высокий уровень ВИП аккаунта!");
		    	    else if(pInfo[playerid][pVip] == 2 && (cInfo[z][cModel] == 400 || cInfo[z][cModel] == 466 || cInfo[z][cModel] == 579)) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"Чтобы сесть в данный транспорт необходим более высокий уровень ВИП аккаунта!");
		    	}
			}
 		}
	    if(GetPVarInt(playerid,"helpengine")==1)
	    {
			SCM(playerid,0xAEAEAEFF,"Используйте клавишу {ffffff}\"L.CTRL\" {AEAEAE}чтобы завести двигатель, {AEAEAE}и {ffffff}\"/rem\" {AEAEAE}чтобы пристегнуть ремень"), SCM(playerid,0xAEAEAEFF,"Чтобы открыть {ffffff}багажник/капот, {AEAEAE}подойдите к нему и нажмите клавишу {ffffff}\"Y\""), DeletePVar(playerid,"helpengine");
			if( PlayerInCAR( playerid ) ) SCM(playerid,0xAEAEAEFF,"Левый поворотник - {ffffff}\"Q\" {AEAEAE}Правый поворотник - {ffffff}\"E\" {AEAEAE}Аварийный сигнал - {ffffff}\"NUM8\"");
			for(new i; i != sizeof sirenpos; i ++) // мигалки
			{
			    if(sirenpos[i][models] == GetVehicleModel(GetPlayerVehicleID(playerid)))
			    {
			    	if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 585)
		    		{
			    	    if(GetPlayerVehicleID(playerid) >= ymvd[0] && GetPlayerVehicleID(playerid) <= ymvd[1]) return SCM(playerid,0xAEAEAEFF,"Чтобы включить/выключить проблесковые маячки, нажмите клавишу {ffffff}\"N\"");
						else return 1;
					}
			    	SCM(playerid,0xAEAEAEFF,"Чтобы включить/выключить проблесковые маячки, нажмите клавишу {ffffff}\"N\"");
			    }
			}
		}
		return 1;
    }
	return 1;
}

public OnPlayerEditDynamicObject( playerid, objectid, response, Float:x, Float:y, Float:z, Float:rx, Float:ry, Float:rz)
{
	new Float: fX = x,
	    Float: fY = y,
	    Float: fZ = z,
	    Float: fRotX = rx,
	    Float: fRotY = ry,
	    Float: fRotZ = rz;

    if( response == EDIT_RESPONSE_FINAL )
	{
	    if( GetPVarInt(playerid,"editobject") == 1 )
		{
			for(new i; i != MAX_OBJECTS_DROP; i ++)
			{
			    if(_objectInfo[i][objDrop][0] == 0 && _objectInfo[i][objDrop][1] == 0)
			    {
					_objectInfo[i][objDrop][0] = _invPlayer[playerid][invSlot][GetPVarInt(playerid,"selectSlot")];
					_objectInfo[i][objDrop][1] = 1; //_invPlayer[playerid][invSlotKol][GetPVarInt(playerid,"selectSlot")];
					_objectInfo[i][objPos][0] = fX;
					_objectInfo[i][objPos][1] = fY;
					_objectInfo[i][objPos][2] = fZ;
					_objectInfo[i][objObject] = objectid;
					_objectInfo[i][objWorld] = GetPlayerVirtualWorld( playerid );

					SetDynamicObjectPos(_objectInfo[i][objObject], fX, fY, fZ);
					SetDynamicObjectRot(_objectInfo[i][objObject], fRotX, fRotY, fRotZ);

					_objectInfo[i][objText] = CDT(_itemsInfo[_invPlayer[playerid][invSlot][GetPVarInt(playerid,"selectSlot")]][invName],0xFFFFFF50, _objectInfo[i][objPos][0],_objectInfo[i][objPos][1],_objectInfo[i][objPos][2],2.0,INVALID_PLAYER_ID,INVALID_VEHICLE_ID,0,GetPlayerVirtualWorld(playerid));

				    _invPlayer[playerid][invSlotKol][GetPVarInt(playerid,"selectSlot")]--;
				    if(_invPlayer[playerid][invSlotKol][GetPVarInt(playerid,"selectSlot")] <= 0) _invPlayer[playerid][invSlot][GetPVarInt(playerid,"selectSlot")]= 0;
				    SaveInventory(playerid);
				    SCM(playerid,0xB2EC5DFF,"Вы успешно положили предмет");
				    DeletePVar(playerid,"editobject");
					DeletePVar(playerid,"selectSlot");
					SCM(playerid, -1, "Чтобы поднять предмет, нажмите клавишу "C_GRAY"- H");

					break;
				}
			}
		}

		if(GetPVarInt(playerid,"editfrakobject")==1)
		{
			for(new i; i != MAX_FRAK_OBJ; i ++)
			{
			    if(fObj[i][objID] == 0 && fObj[i][objFrak] == 0)
				    {
					fObj[i][objPos][0] = fX;
					fObj[i][objPos][1] = fY;
					fObj[i][objPos][2] = fZ;
					fObj[i][objPlayer] = playerid;
					fObj[i][objID] = objectid;
					fObj[i][objFrak] = pInfo[playerid][pFrak];
					SetDynamicObjectPos(objectid, fX, fY, fZ);
					SetDynamicObjectRot(objectid, fRotX, fRotY, fRotZ);

				    SCM(playerid,0xB2EC5DFF,"Вы успешно установили предмет");
				    SCM(playerid,0xB2EC5DFF,"Чтобы удалить предмет введите: /delobj или /fobj - установленные объекты");
				    DeletePVar(playerid,"editfrakobject");

					break;
				}
			}
		}

		if( GetPVarInt(playerid,"fursel") == 1 || GetPVarInt(playerid,"fursel") == 2 ) // установить / отредактировать
		{
		    new
				i = GetPVarInt(playerid,"nmenuid"),
		    	id = GetPVarInt(playerid,"furslot");

			if( GetPlayerVirtualWorld( playerid ) != nInfo[ i ][ nInt ] ) return SCM( playerid, -1, "Вы должны находиться внутри вашей недвижимости! 2 ");
   			if( !IsPlayerInRangeOfPoint( playerid, 40.0, nInfo[ i ][ nExite ][ 0 ], nInfo[ i ][ nExite ][ 1 ], nInfo[ i ][ nExite ][ 2 ] ) )
   			{
   			    if( GetPVarInt(playerid,"fursel") == 1 )
				{
					DestroyDynamicObjected( fInfo[ GetPVarInt(playerid,"nmenuid") ][ fObjectid ][ GetPVarInt( playerid, "furslot" ) ]);
				    fInfo[ GetPVarInt(playerid,"nmenuid") ][ fObjectid ][ GetPVarInt( playerid, "furslot" ) ] = EOS;
				    fInfo[ GetPVarInt(playerid,"nmenuid") ][ fObject ][ GetPVarInt( playerid, "furslot" ) ] = EOS;
				    DeletePVar( playerid, "fursel" );
					SetPlayerPosF( playerid, nInfo[ i ][ nExite ][ 0 ], nInfo[ i ][ nExite ][ 1 ], nInfo[ i ][ nExite ][ 2 ], nInfo[ i ][ nExite ][ 3 ]);
				}

				if( GetPVarInt(playerid,"fursel") == 2 )
				{
					SPD( playerid, dFurniture, list, " ","" #C_GRAY "•{FFFFFF} Отредактировать мебель\n" #C_GRAY "•{FFFFFF} Установить мебель","Ок","Назад");
				    DeletePVar( playerid, "fursel" );
				    SetPlayerPosF( playerid, nInfo[ i ][ nExite ][ 0 ], nInfo[ i ][ nExite ][ 1 ], nInfo[ i ][ nExite ][ 2 ], nInfo[ i ][ nExite ][ 3 ]);
				}
   				return SCM( playerid, -1, "Вы должны находиться внутри вашей недвижимости! 3 ");
			}
			DestroyDynamicObject(fInfo[i][fObjectid][id]);
			
			fInfo[ i ][ fObjectid ][ id ] = CreateDynamicObject(_itemsInfo[_invPlayer[playerid][invSlot][ GetPVarInt(playerid,"selectSlot") ]][invObject],
								fX,
							 	fY,
							  	fZ,
							 	fRotX,
							   	fRotY,
							  	fRotZ,
						 		nInfo[i][nInt],
							 	-1,
							 	-1,
							 	STREAMER_OBJECT_SD,
							 	STREAMER_OBJECT_DD,
						 		-1,
							 	0);
            Streamer_SetIntData(STREAMER_TYPE_OBJECT, fInfo[i][fObjectid][id], E_STREAMER_WORLD_ID, nInfo[i][nInt]);

			_invPlayer[playerid][invSlotKol][GetPVarInt(playerid,"selectSlot")]--;
