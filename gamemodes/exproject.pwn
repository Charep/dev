            }
            else if( steppov[ vehicleid ] == 3 )
            {
                TextDrawColor(Speedmetr6[i], 1515870864); // off
            }

            if( steppov[ vehicleid ] == 6 ) // аварийный сигнал
            {
            	TextDrawColor(Speedmetr6[i], 1220628223); // on
            	TextDrawColor(Speedmetr5[i], 1220628223); // on
            }
            else if( steppov[ vehicleid ] == 5 )
            {
                TextDrawColor(Speedmetr6[i], 1515870864); // off
                TextDrawColor(Speedmetr5[i], 1515870864); // off
            }

            if( steppov[ vehicleid ] <= 0 || steppov[ vehicleid ] >= 7 ) // поворотники выключены
			{
				TextDrawColor(Speedmetr6[i], 1515870864); // off
				TextDrawColor(Speedmetr5[i], 1515870864); // off
			}

			ShowSpeedo(i);
		}
        if(!IsPlayerInAnyVehicle(i)) HideSpeedo(i);
    }
	return 1;
}

