CMD:gotoned(playerid, params[])
{
    if(IsAdmin(playerid, 5)) return 1;

    if( sscanf(params, "d", params[0] ) ) return SCM(playerid, COLOR_WHITE, "Используйте: /gotoned [ид недвижимости]");

    if( params[0] > LoadAllNed || params[0] < 0 ) return SCM( playerid, -1, "Недвижимость с указанным ID не найдена!");

	new
		bool:found = false;

    for(new i; i != LoadAllNed; i++)
	{
		if( i == params[0] )
		{
		    if( nInfo[ i ][ nPick ][ 0 ] != 0.0 )
		    {
		    	SetPlayerPosF( playerid, nInfo[ i ][ nPick ][ 0 ], nInfo[ i ][ nPick ][ 1 ], nInfo[ i ][ nPick ][ 2 ], 0 );
		        SendMes( playerid, -1, "Вы успешно телепортировались к недвижимости №%d",i);
		    }
		    else if( nInfo[ i ][ nBuy ][ 0 ] != 0.0 )
		    {
		        SetPlayerPosF( playerid, nInfo[ i ][ nBuy ][ 0 ], nInfo[ i ][ nBuy ][ 1 ], nInfo[ i ][ nBuy ][ 2 ], 0 );
		        SendMes( playerid, -1, "Вы успешно телепортировались к недвижимости №%d",i);
		    }
		    else
		    {
				SCM( playerid, -1, "У недвижимости с указанным ID не найдены координаты!");
			}

			found = true;

		    break;
		}
	}

	if( found == false ) return SCM( playerid, -1, "Недвижимость с указанным ID не найдена!");

	return 1;
}

