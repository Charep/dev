        new
			bool:found = false,
			count;

    	for(new i; i != LoadAllNed; i++)
	    {
		    if(IsPlayerInRangeOfPoint(playerid,6.0,nInfo[i][nPick][0],nInfo[i][nPick][1],nInfo[i][nPick][2]))
		    {
		        if(sscanf(params, "dd", params[0], params[1])) return SCM(playerid, COLOR_WHITE, "Используйте: /buyfuel [количество канистр] [1 - канистра на 10 литров | 2 - канистра на 20 литров]"), SCM(playerid, COLOR_WHITE, "Цена канистры на 10 литров - 150р | Цена канистры на 20 литров - 250р");

