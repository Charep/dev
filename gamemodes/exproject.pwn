	format:str_small("{FFFFFF}Тип недвижимости: {FFDA00}%s\n",GetPropertyType(i)), strcat(str_cmd, str_small);
	if(nInfo[i][nType]==4 && nInfo[i][nCost]==0) nInfo[i][nCost]=30;
