			if(pInfo[playerid][pPhonemoney] < price) return SCM(playerid,-1,"У Вас не хватает денег на счету, чтобы отправить это SMS сообщение");

			SendMes(i,-1,"{E7DF4B}SMS от %d: %s",pInfo[playerid][pNumber], params[1]);
			SendMes(playerid,-1,"{E7DF4B}SMS для %d: %s " #C_GRAY "( - %d рублей )",pInfo[i][pNumber], params[1], price);

			pInfo[playerid][pPhonemoney] -= price;
