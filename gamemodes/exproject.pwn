	{
	    if( oInfo[ pInfo[i][pOrg][0] ][ oMoney ] > oInfo[ pInfo[i][pOrg][0] ][ oPay ] )
	    {
	    	SendMes(i,-1,"" #C_GRAY "Ваша зарплата от организации %s составила: %d руб", oInfo[ pInfo[i][pOrg][0] ][ oName ], oInfo[ pInfo[i][pOrg][0] ][ oPay ] );
