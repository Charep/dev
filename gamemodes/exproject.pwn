CMD:sms(playerid, params[])
{
    if(GetPVarInt(playerid,"death") == 1 || GetPVarInt(playerid,"death") == 2) return 1;

    if( pInfo[playerid][pArrest] != 0 ) return SCM(playerid, -1,"У Вас забрали все вещи, и вернут только  после окончания вашего заключения");
   	if(pInfo[playerid][pPhoneMute] != 0) return SendClientMessage(playerid,-1,"Ваш телефон недоступен/заблокирован сотовой связью.");

    if(sscanf(params, "ds[170]", params[0], params[1])) return SCM(playerid, COLOR_WHITE, "Используйте: /sms [номер телефона] [текст сообщения]");

    if(strlen(params[1]) <= 0 || strlen(params[1]) > 170) return SCM(playerid,-1,"Текст сообщения от 1 до 170 символов");

    new bool:found = false;

    for( new i; i != 10; i++ )
	{
		if(_invPlayer[playerid][invSlot][i] > 3 && _invPlayer[playerid][invSlot][i] < 13)
		{
			found = true;
			break;
		}
	}

	if(found == false) return SCM(playerid,-1,"В Вашем инвентаре нет телефона!");

	found = false;

	foreach(Player, i)
	{
		if(pInfo[i][pNumber]!=params[0]) continue;
		if(pInfo[i][pNumber]==params[0])
		{
		    new price = floatround( strlen(params[1]) * 0.5, floatround_ceil);
