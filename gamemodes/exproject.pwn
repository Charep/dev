CMD:r(playerid, params[])
{
    if(pInfo[playerid][pFrak][0] != 0)
    {
		if(sscanf(params, "s[170]", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /r [IC рация фракции]");

		if( strlen( params[0] ) > 170 || strlen( params[0] ) < 0 ) return SCM(playerid, -1, "Сообщение не должно превышать 170 символов");

		foreach(Player, i)
		{
		    if( pInfo[ i ][ pAdmin ] != 0 )
		    {
		        if( GetPVarInt( i, "rspec" ) == pInfo[playerid][pFrak][0] )
		        {
		    		format:str_small("" #C_LIGHTBLUE "[R] %s %s[%d]: %s",pInfo[playerid][pDol],pInfo[playerid][pName],playerid,params[0]);
					SAM(COLOR_WHITE, str_small);
					str_small[0] = EOS;
				}
			}

   			if(pInfo[i][pFrak][0] != pInfo[playerid][pFrak][0]) continue;
			SendMes(i,0x4C9DBFFF,"[R] %s %s[%d]: %s",pInfo[playerid][pDol],pInfo[playerid][pName],playerid,params[0]);
		}
		format:str_small("{AF5DD2}Говорит в рацию:{ffffff} %s", params[0]);
        SetPlayerChatBubble(playerid, str_small, COLOR_PURPLE, 10.0, 5000);
		return 1;
	}
	return SCM(playerid,-1,"У Вас нет доступа к данной команде!");
}
