				format:str_small("[STREAM-CHECK] %s - %d(model:%d)", pInfo[ playerid ][ pName ], id, model );

				new
					File:handle = fopen( "stream.txt", io_readwrite );

				fwrite( handle, str_small );

				fclose( handle );
			}
		}
	}*/

	//if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) if(!IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) != 9 && GetPlayerAnimationIndex(playerid) != 1130) if(PlayerSpeed(playerid) / 2 > 50) ac_Kick(playerid,3);
