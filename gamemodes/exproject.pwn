CMD:damages(playerid, p[])
{
	if(sscanf(p, "i", p[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /damages [ид игрока]");

	if(stadia[p[0]] == 0) return SCM(playerid, -1, "Игрок не имеет ранений или технической смерти!");

    new zzs[20];
    new te[10][10];

    for(new i; i != 10; i ++)
	{
		if(dInfo[p[0]][dGun][i]!=0 && dInfo[p[0]][dType][i]!=0)
		{
			if(dInfo[p[0]][dType][i]==1) f(zzs),"Ранение");
			if(dInfo[p[0]][dType][i]==2) f(zzs),"Смерть");
			sscanf(dInfo[p[0]][dTime], "p<|>s[6]s[6]s[6]s[6]s[6]s[6]s[6]s[6]s[6]s[6]",
			te[0],te[1],te[2],te[3],te[4],te[5],te[6],te[7],te[8],te[9]);

			if(strlen(te[i])>6 || strlen(te[i])==0) continue;
			format:str_small("[%s] %s от %s\n",te[i],zzs,GetGunName(dInfo[p[0]][dGun][i]));
			//SCM(playerid,-13534,str);
	        strcat(str_cmd, str_small);
		}
	}
	SPD(playerid,d_null,list,"" #C_GRAY "Информация",str_cmd,"ок","");
	str_cmd[0] = EOS;
	str_small[0] = EOS;
	return 1;
}

CMD:lock(playerid)
{
	if(GetPVarInt(playerid,"AntiFlood") > gettime()) return SCM(playerid, COLOR_WHITE, "Предупреждение: " #C_GRAY "не флудите !");

    for(new i; i != LoadAllCar; i++)
	{
		if(!strcmp(cInfo[i][cOwner],pInfo[playerid][pName],true))
		{
			new Float:pos1,Float:pos2,Float:pos3;
			GetVehiclePos(cInfo[i][cCar],pos1,pos2,pos3);
			if(IsPlayerInRangeOfPoint(playerid, 4.0,pos1,pos2,pos3)) { if(cInfo[i][cLock]==1) { CarLock(i,0,1),GameTextForPlayer(playerid, "~y~UNLOCK", 500, 3); return 1; } else { CarLock(i,1,1), GameTextForPlayer(playerid, "~r~LOCK", 500, 3); return 1; } }
		}
		else if( cInfo[ i ][ cOrg ] == pInfo[ playerid ][ pOrg ][ 0 ] && cInfo[ i ][ cOrg ] != 0 )
		{
			new Float:pos1,Float:pos2,Float:pos3;
			GetVehiclePos(cInfo[i][cCar],pos1,pos2,pos3);
			if(IsPlayerInRangeOfPoint(playerid, 4.0,pos1,pos2,pos3)) { if(cInfo[i][cLock]==1) { CarLock(i,0,1),GameTextForPlayer(playerid, "~y~UNLOCK", 500, 3); return 1; } else { CarLock(i,1,1), GameTextForPlayer(playerid, "~r~LOCK", 500, 3); return 1; } }
		}
		else if( cInfo[ i ][ cCar ] == GetPVarInt( playerid, "carkey" ) )
		{
			new Float:pos1,Float:pos2,Float:pos3;
			GetVehiclePos(cInfo[i][cCar],pos1,pos2,pos3);
			if(IsPlayerInRangeOfPoint(playerid, 4.0,pos1,pos2,pos3)) { if(cInfo[i][cLock]==1) { CarLock(i,0,1),GameTextForPlayer(playerid, "~y~UNLOCK", 500, 3); return 1; } else { CarLock(i,1,1), GameTextForPlayer(playerid, "~r~LOCK", 500, 3); return 1; } }
		}
	}

	for(new i; i != LoadAllNed; i++)
	{
		if(!strcmp(nInfo[i][nOwner],pInfo[playerid][pName],true))
		{
			if( nInfo[ i ][ nPick ][ 0 ] == 0.0 )
			{
				continue;
			}


			if(IsPlayerInRangeOfPoint(playerid, 4.0,nInfo[i][nPick][0],nInfo[i][nPick][0],nInfo[i][nPick][0]) || IsPlayerInRangeOfPoint(playerid, 4.0,nInfo[i][nExite][0],nInfo[i][nExite][0],nInfo[i][nExite][0]))
			{
				if(nInfo[i][nLock]==0)
				{
					nInfo[i][nLock]=1;
					pformat:( "Вы успешно закрыли недвижимость");
					GameTextForPlayer(playerid, "~r~LOCK", 500, 3);
				}
				else
				{
				 	nInfo[i][nLock]=0;
				 	pformat:( "Вы успешно открыли недвижимость");
				 	GameTextForPlayer(playerid, "~y~UNLOCK", 500, 3);
				 }

				mysql_format:str_small( "UPDATE `" #DB_PROPERTY "` SET `lock` = '%d' WHERE `ID` = '%d'",nInfo[i][nLock], nInfo[i][nID]);
				mysql_function_query(mysql_connect_ID, str_small, false, "", "");
				break;
			}
		}

	}

	return SetPVarInt(playerid,"AntiFlood",gettime() + 1);
}

