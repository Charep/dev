CMD:eject(playerid, params[])
{
    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /eject [ид игрока]");
    if(!IsPlayerConnected(params[0])) return SCM(playerid,-1,"Игрока с таким ID нет на сервере");
	if(GetPlayerVehicleSeat(playerid)!=0) return SCM(playerid,-1,"Вы должны находиться за рулем автомобиля");
	if(GetPlayerVehicleID(params[0])!=GetPlayerVehicleID(playerid)) return SCM(playerid,-1,"Игрок не находиться в вашем тс");
	return RemovePlayerFromVehicle(params[0]), SendMes(params[0],-1,"{D8D8D8}%s выгнал вас из транспорта",pInfo[playerid][pName]), SendMes(params[0],-1,"{D8D8D8}Вы выгнали %s из транспорта",pInfo[params[0]][pName]);
}

