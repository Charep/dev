CMD:sd(playerid, params[])
{
	if( sscanf(params, "ds[128]", params[0], params[1]) ) return SCM(playerid, COLOR_WHITE, "Используйте: /sd [номер недвижимости] [текст]");
	if( params[0] > LoadAllNed || params[0] < 0 ) return SCM( playerid, -1, "Указанный номер недвижимости - не найден!");
	if( strlen( params[1] ) > 128 || !strlen( params[1] ) ) return SCM( playerid, -1, "Максимальная длина текста - 128 символов!");
	if(IsPlayerInRangeOfPoint( playerid, 10.0, nInfo[ params[0] ][ nPick ][ 0 ], nInfo[ params[0] ][ nPick ][ 1 ], nInfo[ params[0] ][ nPick ][ 2 ]))
	{
	    format:str_small("[(Ваш)Голос с улицы]: %s",params[1]);
	    SCM( playerid, -1, str_small );

	    format:str_small("[Голос с улицы]: %s",params[1]);
		foreach( Player, i )
		{
		    if(IsPlayerInRangeOfPoint( i, 10.0, nInfo[ params[0] ][ nExite ][ 0 ], nInfo[ params[0] ][ nExite ][ 1 ], nInfo[ params[0] ][ nExite ][ 2 ]))
		    {
		        SCM( i, -1, str_small );
		    }
		}
		return 1;
	}
	else if(IsPlayerInRangeOfPoint( playerid, 10.0, nInfo[ params[0] ][ nExite ][ 0 ], nInfo[ params[0] ][ nExite ][ 1 ], nInfo[ params[0] ][ nExite ][ 2 ]))
	{
		format:str_small("[(Ваш)Голос из помещения]: %s",params[1]);
		SCM( playerid, -1, str_small );

		format:str_small("[Голос из помещения]: %s",params[1]);
		foreach( Player, i )
		{
		    if(IsPlayerInRangeOfPoint( i, 10.0, nInfo[ params[0] ][ nPick ][ 0 ], nInfo[ params[0] ][ nPick ][ 1 ], nInfo[ params[0] ][ nPick ][ 2 ]))
		    {
		        SCM( i, -1, str_small );
		    }
		}
		return 1;
	}

	return SCM(playerid,-1,"Вы должны находиться возле выхода или входа указанной недвижимости!");
}

CMD:findned(playerid, params[])
{
	if(pInfo[playerid][pFrak][0] == 1 || pInfo[playerid][pFrak][0]==2)
	{
		if( sscanf(params, "d", params[0]) ) return SCM(playerid, COLOR_WHITE, "Используйте: /findned [номер недвижимости]");
		if( params[0] > LoadAllNed || params[0] < 0 ) return SCM( playerid, -1, "Указанный номер недвижимости - не найден!");

		if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,"Вы должны находиться в транспорте МВД!");
		new car = GetPlayerVehicleID(playerid);
	    if(car < ymvd[0] && car > ymvd[1]) return SCM(playerid,-1,"Вы должны находиться в транспорте МВД!");
	    if(car < omon[0] && car > omon[1]) return SCM(playerid,-1,"Вы должны находиться в транспорте МВД!");

		new
			i = params[0];

	    if( nInfo[ i ][ nPick ][ 0 ] == 0.0 ) return SCM( playerid, -1, "Недвижимость не найдена!");

        SendMes(playerid,-1,"{A9F046}У вас на карте было отмечено: %s №%d", GetPropertyType( i ), i);
		SetPlayerCheckpoint(playerid, nInfo[ i ][ nPick ][ 0 ], nInfo[ i ][ nPick ][ 1 ], nInfo[ i ][ nPick ][ 2 ], 5.0);
	 	SCM(playerid,-1,"Чтобы выключить GPS-навигатор, введите /gps еще раз"),
	 	SetPVarInt(playerid,"gps",1);
	 	return 1;
	}
	else return SCM( playerid, -1, "У Вас нет доступа к данной команде!");
}

CMD:kd(playerid, params[])
{
	if(pInfo[playerid][pFrak][0] == 1 || pInfo[playerid][pFrak][0]==2 || pInfo[playerid][pFrak][0]==8)
	{
		if( sscanf(params, "d", params[0]) ) return SCM(playerid, COLOR_WHITE, "Используйте: /kd [номер недвижимости]");
		if( params[0] > LoadAllNed || params[0] < 0 ) return SCM( playerid, -1, "Указанный номер недвижимости - не найден!");
		if(IsPlayerInRangeOfPoint( playerid, 5.0, nInfo[ params[0] ][ nPick ][ 0 ], nInfo[ params[0] ][ nPick ][ 1 ], nInfo[ params[0] ][ nPick ][ 2 ]) || IsPlayerInRangeOfPoint( playerid, 10.0, nInfo[ params[0] ][ nExite ][ 0 ], nInfo[ params[0] ][ nExite ][ 1 ], nInfo[ params[0] ][ nExite ][ 2 ]))
		{
		    if( nInfo[ params[0] ][ nLock ] == 0 ) return SCM( playerid, -1, "Недвижимость уже открыта!");
		    nInfo[ params[0] ][ nLock ] = 0;
		    foreach( Player, i )
			{
				if(IsPlayerInRangeOfPoint( i, 10.0, nInfo[ params[0] ][ nPick ][ 0 ], nInfo[ params[0] ][ nPick ][ 1 ], nInfo[ params[0] ][ nPick ][ 2 ]) || IsPlayerInRangeOfPoint( i, 10.0, nInfo[ params[0] ][ nExite ][ 0 ], nInfo[ params[0] ][ nExite ][ 1 ], nInfo[ params[0] ][ nExite ][ 2 ]))
				{
				    format:str_small("* %s физической силой или с помощью специальных средств выбил входную дверь", pInfo[ playerid ][ pName ] );
				    SCM( i, 0xAF5DD2FF, str_small );
				}
			}
			cmd::akick(playerid);
			return 1;
		}

		return SCM(playerid,-1,"Вы должны находиться возле выхода или входа указанной недвижимости!");
	}
	else return SCM( playerid, -1, "У Вас нет доступа к данной команде!");
}

