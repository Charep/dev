CMD:delobj(playerid)
{
	if(pInfo[playerid][pFrak]==0) return SCM(playerid,-1,"Данная функция Вам не доступна!");

	for(new i; i != MAX_FRAK_OBJ; i ++)
	{
		if(fObj[i][objFrak] == pInfo[playerid][pFrak])
		{
			if(IsPlayerInRangeOfPoint(playerid,3.0,fObj[i][objPos][0],fObj[i][objPos][1],fObj[i][objPos][2]))
			{
				DestroyDynamicObjected(fObj[i][objID]); SCM(playerid,-1,"Вы удалили объект");
				fObj[i][objID]=0;
				fObj[i][objPlayer]=0;
				fObj[i][objFrak]=0;
				fObj[i][objPos][0]=0.0;
				fObj[i][objPos][1]=0.0;
				fObj[i][objPos][2]=0.0;
				break;
			}
		}
	}

	return 1;
}

