    if(IsAdmin(playerid, 2)) return 1;
    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /unmute [ид]");
    if( !IsPlayerLogged( params[0] ) ) return SCM( playerid, COLOR_DARK_GRAY, !INCORRECT_PLAYER_ID );

   	format:str_cmd( "%s снял блокировку чата игроку %s", CheckAdmin(playerid),pInfo[params[0]][pName] );
	SendClientMessageToAll(COLOR_LIGHT_RED,str_cmd);
	SetPVarInt(params[0],"mute",0);
	mysql_format:str_small("UPDATE `" #DB_ACCOUNTS "` SET `Mute` = '%d' WHERE `Name` = '%s'",params[1], pInfo[params[0]][pName]); mysql_function_query(mysql_connect_ID, str_small, false, "", "");

	new fmt_str[128];
	format(fmt_str, sizeof fmt_str, "Снял бан чата %s", pInfo[params[0]][pName]);
	SendLog(playerid, LOG_TYPE_ADMIN, fmt_str);
	return 1;
}

CMD:kick(playerid, params[])
{
    if(IsAdmin(playerid, 2) || pInfo[playerid][pYouTube] == 1) return 1;
    if(sscanf(params, "us[64]", params[0],params[1])) return SCM(playerid, COLOR_WHITE, "Используйте: /kick [ид] [причина]");

	format:str_cmd( "%s отсоединил от сервера игрока %s. Причина: %s",CheckAdmin(playerid),pInfo[params[0]][pName], params[1]);
	SendClientMessageToAll(COLOR_LIGHT_RED,str_cmd);

	new fmt_str[128];
	format(fmt_str, sizeof fmt_str, "Отключил игрока %s | Причина: %s", pInfo[params[0]][pName], params[1]);
	SendLog(playerid, LOG_TYPE_ADMIN, fmt_str);
    Kick(params[0]);
	return 1;
}

CMD:checkip(playerid, params[])
{
    if(IsAdmin(playerid, 3)) return 1;
    if(pInfo[playerid][pYDostup] == 1) return SCM(playerid,CG1,"Включить отображение IP-адресов для доступа! (/showip)");
    if( sscanf(params, "u", params[0] ) ) return SCM(playerid, COLOR_WHITE, "Используйте: /checkip [ид игрока]");

	new ip[17], country[80], isp[32], city[32], str[800];
	new country1[32], isp1[32], city1[32];
	new country2[32], isp2[32], city2[32];
	GetPlayerIp( params[0], ip, 17 );
	// Рег.IP
	GetIPCountry(pInfo[params[0]][pRegip], country2, sizeof country2);
 	GetIPCity(pInfo[params[0]][pRegip], city2, sizeof city2);
 	GetIPISP(pInfo[params[0]][pRegip], isp2, sizeof isp2);
 	//Last.IP
 	GetIPCountry(pInfo[params[0]][pLastip], country1, sizeof country1);
  	GetIPCity(pInfo[params[0]][pLastip], city1, sizeof city1);
 	GetIPISP(pInfo[params[0]][pLastip], isp1, sizeof isp1);
 	//IP
 	GetIPCountry(ip, country, sizeof country);
 	GetIPCity(ip, city, sizeof city);
 	GetIPISP(ip, isp, sizeof isp);
 	SCM(playerid, -1, "Загружаем информацию о игроке! Подождите немного...");
	format(str, sizeof(str), "\
	IP при регистрации: %s\n\
	Страна: %s\n\
	Город: %s\n\
	Провайдер: %s\n\
	==============================\n\
	IP при последнем находе: %s\n\
	Страна: %s\n\
	Город: %s\n\
	Провайдер: %s\n\
	==============================\n\
	IP сейчас: %s\n\
	Страна: %s\n\
	Город: %s\n\
	Провайдер: %s",
	pInfo[params[0]][pRegip], country2, city2, isp2,
	pInfo[params[0]][pLastip], country1, city1, isp1,
	ip, country, city, isp);
	ShowPlayerDialog(playerid, 0, DIALOG_STYLE_MSGBOX, "{FFCC00}Информация об IP", str, "Принять", "");
	return 1;
}

CMD:checkcar(playerid, params[])
{
    if(IsAdmin(playerid, 2)) return 1;

    if( sscanf(params, "u", params[0] ) ) return SCM(playerid, COLOR_WHITE, "Используйте: /checkcar [ид игрока]");

	new kolvo;

    for(new i; i != LoadAllCar; i++)
	{
		if(!strcmp(cInfo[i][cOwner],pInfo[ params[0] ][pName],true))
		{
			kolvo++;
			format:str_small("" #C_GRAY "%d.{ffffff} %s [%s] - id %d\n",kolvo,GetCarMark(cInfo[i][cModel]),cInfo[i][cNumber], cInfo[i][cCar]);
			if(cInfo[i][cArrest]==1) format:str_small("" #C_GRAY "%d.{ffffff} %s [%s] - {992F2F}Ш/С\n",kolvo,GetCarMark(cInfo[i][cModel]),cInfo[i][cNumber]);
			strcat(str_big,str_small);
		}
	}

	format:str_least("" #C_GRAY "Транспорт игрока %s", pInfo[ params[0] ][ pName ] );

	SPD( playerid, d_null, list, str_least, str_big, "Закрыть", "");

	str_big[0] = EOS;
	ClearVar();

	return 1;
}

CMD:checkned(playerid, params[])
