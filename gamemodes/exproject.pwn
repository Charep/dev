CMD:mask(playerid)
{
    new bool:found = false, item;

    if( pInfo[playerid][pArrest] != 0 ) return SCM(playerid, -1,"У Вас забрали все вещи, и вернут только  после окончания вашего заключения");

    for( new i; i != 10; i++ )
	{
		if(_invPlayer[playerid][invSlot][i] == 378) found = true, item = i;
	}

	if(found == true)
	{
	    if(GetPVarInt(playerid,"mask") != 0) return SCM(playerid,-1,"На Вас уже надета маска. Используйте команду "C_GRAY"/maskoff, "C_WHITE"чтобы снять ее");
		_invPlayer[playerid][invSlotKol][ item ]--;
	    if(_invPlayer[playerid][invSlotKol][ item ] <= 0) _invPlayer[playerid][invSlot][ item ]= 0;
	    SaveInventory(playerid);
	    new r = random (19999)+1;
	    SetPVarInt(playerid,"mask",r);
	    format:str_least(""C_WHITE"Маска "C_GRAY"[%d]",r);
	    mask[playerid] = CreateDynamic3DTextLabel(str_least, 0xFFFFFFFF, 0.0, 0.0, 0.4, 8.0, playerid, INVALID_VEHICLE_ID,1,-1,-1,-1,9.0);
		SetPlayerChatBubble(playerid,"Надел маску",0xAF5DD2FF,20.0,7000);
		SCM(playerid,-1,"Используйте команду "C_GRAY"/maskoff, "C_WHITE"чтобы снять маску");
		new animlib[32];
	    new animname[32];
		GetAnimationName(834,animlib,32,animname,32);
	    ApplyAnimation(playerid,animlib,animname,8.1, 0, 1, 1, 0, 0);
	    foreach(Player, i) ShowPlayerNameTagForPlayer(i,playerid,0);
	}
