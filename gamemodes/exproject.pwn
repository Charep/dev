CMD:loadcnum(playerid, params[])
{
    if(IsAdmin(playerid, 3)) return 1;
	if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /loadcnum [id транспота] (Обновить номера автомобиля)");

	new i=-1;

    for( new z; z != LoadAllCar; z++ )
	{
		if(cInfo[z][cCar]==params[0]) { i=z; break; }
	}

	if(i==-1)
	{
		return SCM(playerid,-1,"Автомобиль не найден!");
	}

	if(!strcmp(cInfo[i][cNumber],"none",true))
	{
		return SCM(playerid,-1,"У автомобиль нет номерных знаков!");
	}

    if(strcmp(cInfo[i][cNumber],"none",true))
	{
        DestroyDynamic3DVehicle( cInfo[i][cText], cInfo[i][cCar] );
		cInfo[i][cText] = CreateDynamic3DTextLabel(cInfo[i][cNumber], 0xFFFFFFFF, 0.0, 0.0, 1.1, 15.0, INVALID_PLAYER_ID, cInfo[i][cCar],1,-1,-1,-1,16.0);
		return SCM(playerid,-1,"Вы успешно обновили номерные знаки автомобиля!");
	}
	return 1;
}

