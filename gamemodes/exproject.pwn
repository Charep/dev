CMD:call(playerid, params[])
{
    if(GetPVarInt(playerid,"death") == 1 || GetPVarInt(playerid,"death") == 2) return 1;

    if( pInfo[playerid][pArrest] != 0 ) return SCM(playerid, -1,"У Вас забрали все вещи, и вернут только  после окончания вашего заключения");

    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /call [номер телефона]");

    new bool:found = false;

    for( new i; i != 10; i++ )
	{
		if(_invPlayer[playerid][invSlot][i] > 3 || _invPlayer[playerid][invSlot][i] < 13) found = true;
	}

	if(found == true)
	{
		if(GetPVarInt(playerid,"goodok")!=0 || GetPVarInt(playerid,"zvon1")!=0 || GetPVarInt(playerid,"zvon2")!=0) return SCM(playerid,-1,"Вы уже разговариваете по другой линии");

		if(pInfo[playerid][pPhonemoney]<2) return SCM(playerid,-1,"На счету вашего телефона недостаточно средств для совершения вызова");

		if(params[0]==101)
		{
			return SPD(playerid,dPhone+3,input,"" #C_GRAY "Экстренные службы | бесплатно","{ffffff}Введите сообщение для МЧС - 101","Ок","Закрыть");
		}
		if(params[0]==102)
		{
			return SPD(playerid,dPhone+4,input,"" #C_GRAY "Экстренные службы | бесплатно","{ffffff}Введите сообщение для полиции - 102","Ок","Закрыть");
		}
		if(params[0]==103)
		{
			return SPD(playerid,dPhone+5,input,"" #C_GRAY "Экстренные службы | бесплатно","{ffffff}Введите сообщение для скорой помощи - 103","Ок","Закрыть");
		}

		if(params[0]==777)
		{
			if(GetPVarInt(playerid,"taxi")==1 && GetPVarInt(playerid,"taxitype")!=0) return SCM(playerid,-1,"Пока вы находитесь на дежурстве, вы не можете заказать такси");
			for(new id; id < MAX_VIZ; id ++)
			{
				if(viInfo[id][viPlayerid]==playerid && viInfo[id][viEnter]==1)
				{
					pformat:( "{FFD918}Вы уже оставили свой заказ такси {ffffff}(№%d)",id);
					psend:( playerid, COLOR_WHITE );
					return 1;
				}
			}
			return SPD(playerid,dPhone+2,input,"" #C_GRAY "Вызов такси","{FFFFFF}Опишите место, где вы находитесь:\n  - Объем до 70 символов","Вызвать","Закрыть");
		}

		if(params[0]==344)
		{
			return SPD(playerid,dPhone+6, input,"" #C_GRAY "Экстренные службы | бесплатно","{ffffff}Введите сообщение для СТО - 344","Ок","Закрыть");
		}

		if(strlen(params[0])!=6) return SCM(playerid,-1,"Номер телефона должен иметь формат из 6 цифр");

		if( c_info[playerid][cStatus] != 0 ) SCM(playerid,-1,"На ваш номер телефона кто-то звонит или вы уже разговариваете по телефону!");

		found = false;

		foreach(Player, i)
		{
			if(pInfo[i][pNumber]!=params[0]) continue;
			if(pInfo[i][pNumber]==params[0])
			{
				if( c_info[i][cStatus] != 0 )
				{
					PlayAudioStreamForPlayer(playerid, "http://exrptest.myarena.ru/Tele.mp3");
					pformat:( "{FFDE00}Номер абонента %d - занят!",pInfo[i][pNumber]);
					psend:( playerid, COLOR_WHITE );
					found = true;
					break;
				}

				pformat:( "{FFDE00}На ваш мобильный телефон идет вызов от абонента %d",pInfo[playerid][pNumber]);
				psend:( i, COLOR_WHITE );
				SCM(i,-1,"{FFDE00}Чтобы принять вызов, введите /p или отклонить /c");

				c_info[playerid][cCaller] = playerid;
				c_info[playerid][cAnswerer] = i;
				c_info[playerid][cStatus] = 1;

				c_info[i][cCaller] = playerid;
				c_info[i][cAnswerer] = i;
				c_info[i][cStatus] = 2;

				pformat:( "{FFDE00} Вы позвонили на номер %d",pInfo[i][pNumber]);
				psend:( playerid, COLOR_WHITE );
				SCM(playerid,-1,"Чтобы сбросить звонок, введите /c");

				PlayAudioStreamForPlayer(playerid, "http://exrptest.myarena.ru/goodok.mp3");

				found = true;
				SetPVarInt(playerid,"goodok",25);

				break;
			}
		}

		if( found == false ) SCM(playerid,-1,"Указанный номер - не найден!");

		return 1;
	}
