	return 1;
}

CMD:checkmask(playerid, params[0])
{
	if(IsAdmin(playerid, 2)) return 1;

	if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /checkmask [ид маски]");

	new bool:found = false;

    foreach(Player, i)
    {
        if(GetPVarInt(i,"gLogged") == 0) continue;
        if(GetPVarInt(i,"mask") == params[0])
        {
			found = true;
			SendMes(playerid,-1,""C_GRAY"Маска %d "C_WHITE"- %s",params[0],pInfo[i][pName]);
			break;
        }
    }

	if(found == false) return SCM(playerid,-1,"Игрок с указанным номером маски - не найден!");
	return 1;
}


CMD:pay(playerid, params[])
{
    if(sscanf(params, "dd", params[0],params[1])) return SCM(playerid, COLOR_WHITE, "Используйте: /pay [ид игрока, которому вы хотите передать свои деньги] [количество денег]");
	if(pInfo[playerid][pGame]<4) return SCM(playerid,-1,"Чтобы передавать деньги, вам нужно отыграть на сервере 4 часа");
	if(!IsPlayerConnected(params[0])) return SCM(playerid,-1,"Игрока с таким ID нет на сервере");
	if(params[0]==playerid) return SCM(playerid, COLOR_WHITE, "Вы не можете выполнить это действие");
	if(GetPVarInt(params[0],"spec")==1) return SCM(playerid,-1,"Вы должны находиться возле игрока, которому хотите передать деньги");
    new Float:p1,Float:p2,Float:p3; GetPlayerPos(params[0],p1,p2,p3);
	if(!IsPlayerInRangeOfPoint(playerid, 10.0,p1,p2,p3)) return SCM(playerid,-1,"Вы должны находиться возле игрока, которому хотите передать деньги");
	if(params[1]<1 || params[1]>50000) return SCM(playerid,-1,"Максимум вы можете передать от 1 до 50.000 рублей");
	if(pInfo[playerid][pMoney]<params[1]) return SCM(playerid,-1,"У вас нет столько денег");
	pInfo[playerid][pMoney]-=params[1]; SaveMoney(playerid);
	pInfo[params[0]][pMoney]+=params[1]; SaveMoney(params[0]);
	SetPlayerChatBubble(playerid, "передал деньги", 0xAF5DD2FF,20.0,9000);
	format:str_least("%s передал деньги %s",pInfo[playerid][pName], pInfo[params[0]][pName]);
	ProxDetector(15.0,playerid,str_least,0xAF5DD2FF,0xAF5DD2FF,0xAF5DD2FF,0xAF5DD2FF,0xAF5DD2FF);
	SendMes(params[0], -1,"%s передал Вам %dр",pInfo[playerid][pName], params[1]);
	if(pInfo[playerid][pYDostup] == 0)
	{
		format:str_small("" #C_LIGHTBLUE "[PAY] Игрок %s[%d](%s) передал %d денег игроку %s[%d](%s)", pInfo[ playerid ][ pName ], playerid, pInfo[playerid][pIP], params[1], pInfo[ params[0] ][ pName], params[0], pInfo[params[0]][pIP] );
	}
	else
	{
	    format:str_small("" #C_LIGHTBLUE "[PAY] Игрок %s[%d](СКРЫТ) передал %d денег игроку %s[%d](СКРЫТ)", pInfo[ playerid ][ pName ], playerid, params[1], pInfo[ params[0] ][ pName], params[0] );
	}
	SAMT(COLOR_WHITE, str_small, 1);
	str_small[0] = EOS;

	return 1;
}

