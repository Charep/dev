		cache_get_field_content ( i, "search_name", name, mysql_connect_ID, MAX_PLAYER_NAME );
		cache_get_field_content ( i, "search_text", text, mysql_connect_ID, 30 );

		format:str_small(""C_WHITE"№%d [%s]: "C_GRAY"%s - %d - %s...\n",
		cache_get_field_content_int ( i, "search_id", mysql_connect_ID ),
		timec( cache_get_field_content_int ( i, "search_date", mysql_connect_ID ) ),
		name,
		cache_get_field_content_int ( i, "search_level", mysql_connect_ID ),
		text );

		strcat( str_big, str_small );
	}

	if( count == 0 )
	{
		SCM(playerid, -1,"База данных - пуста!");
		SPD( playerid, dFrak+15, list, ""C_GRAY"База данных МВД", search_text, "Ок", "Закрыть" );
		return 1;
	}

	if( count == 26 ) return SPD(playerid, dSearchPlayers+1, info, ""C_GRAY"База разыскиваемых граждан", str_big, "Назад", "Далее");
	return SPD(playerid, dSearchPlayers+1, info, ""C_GRAY"База разыскиваемых граждан", str_big, "Назад", "");
}

