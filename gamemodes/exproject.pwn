 		mysql_function_query(mysql_connect_ID, str_small, false, "", "");

		ResetPlayerWeapons( params[0] );
		DeletePVar( params[0], "duty" );
        SaveWeapon( params[0] );

		return 1;
	}

	if(pInfo[playerid][pOrg][0]>0)
	{
	    if(strcmp(oInfo[ pInfo[playerid][pOrg][0] ][oLeader],pInfo[playerid][pName],true)) return SCM(playerid,-1,"Вы не являетесь лидером!");
	    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /uninvite [ид игрока]");
		if(params[0]==playerid) return SCM(playerid, COLOR_WHITE, "Вы не можете принять самого себя");
		if(pInfo[params[0]][pOrg][0]!=pInfo[playerid][pOrg][0]) return SCM(playerid, COLOR_WHITE, "Игрок состоит в другой организации");
		if(pInfo[params[0]][pFrak][0]!=0) return SCM(playerid, COLOR_WHITE, "Игрок состоит в другой фракции");
		if(GetPVarInt(params[0],"gLogged")!=1) return SCM(playerid, COLOR_WHITE, "Игрок не найден!");

		pformat:( "" #C_GRAY "Вы уволили из организации игрока {ffffff}%s",pInfo[params[0]][pName]);
		psend:( playerid, COLOR_WHITE );

		pformat:( "" #C_GRAY "Лидер {ffffff}%s " #C_GRAY "уволил вас из организации {ffffff}%s !",pInfo[playerid][pName], oInfo[pInfo[playerid][pOrg][0]][oName]);
		psend:( params[0], COLOR_WHITE );

		pInfo[params[0]][pOrg][0]=-1;
		pInfo[params[0]][pOrg][1]=0;

		mysql_format:str_small("UPDATE `" #DB_ACCOUNTS "` SET `Org` = '0', `Orang` = '0' WHERE `ID` = '%d'",pInfo[params[0]][pID]);
		mysql_function_query(mysql_connect_ID, str_small, false, "", "");

		return 1;
	}

	return 1;
}

