CMD:cdo(playerid, params[])
{
	if(cdo[GetPVarInt(playerid,"cdocar")] == true)
	{
		DestroyDynamic3DVehicle( cdotext[ GetPVarInt(playerid, "cdocar") ], GetPVarInt(playerid, "cdocar") );
		SCM( playerid, -1, "Вы сняли 3D текст с автомобиля" );
		cdo[ GetPVarInt(playerid, "cdocar") ] = false;
		DeletePVar( playerid, "cdocar" );
		return 1;
	}

	if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,"Вы должны находиться в Т/С");

	for(new i; i != LoadAllCar; i++)
	{
		if(GetPlayerVehicleID(playerid)==cInfo[i][cCar])
		{
			if(strcmp(cInfo[i][cOwner],pInfo[playerid][pName],true))
			{
				return SCM(playerid,-1,"Вы не можете установить описание на чужое личное Т/С");
			}
			else if( cInfo[ i ][ cOrg ] == pInfo[ playerid ][ pOrg ][ 0 ] && cInfo[ i ][ cOrg ] != 0 )
			{
				break;
			}
		}
	}


	if(sscanf(params, "s[150]", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /cdo [Описание вашего Т/С и его особенностей]");
    if(strlen(params[0]) > 150 || strlen(params[0]) < 1) return SCM(playerid, -1,"Описание не меньше 1 и не больше 150 символов");
    format:str_small("%s", params[0], pInfo[playerid][pName],playerid);
    cdotext[GetPlayerVehicleID(playerid)] = CreateDynamic3DTextLabel(str_small, 0xBA4FA5FF, 0.0, 0.0, 1.0, 15.0, INVALID_PLAYER_ID, GetPlayerVehicleID(playerid),1,-1,-1,-1,16.0);
    format:str_cmd("{E2F43D}[CDO] %s[%d] {ffffff}%s",pInfo[playerid][pName],playerid,params[0]);
    SAM(COLOR_WHITE, str_cmd);
    return SCM(playerid,0xBA4FA5FF,params[0]), cdo[GetPlayerVehicleID(playerid)] = true, SetPVarInt(playerid,"cdocar", GetPlayerVehicleID(playerid)), SCM(playerid,-1, "Вы установили 3D текст описания авто. Чтобы убрать его, введите команду повторно");
}


