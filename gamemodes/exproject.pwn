stock SetVehicleSpeed(vehicleid,modelid)
{
	new Float:Vx, Float:Vy, Float:Vz ,Float:speeds, Float:mph, Float:upvyz, Float:maxscor;
	GetVehicleVelocity(vehicleid,Vx,Vy,Vz);
	//speeds = floatsqroot(Vx*Vx + Vy*Vy + Vz*Vz);
	speeds = floatround(floatsqroot(((Vx*Vx)+(Vy*Vy))+(Vz*Vz)));
	switch(modelid)
	{
		case 400:
		{
			mph = 250;
			upvyz = 0.25;
			maxscor = 90;
		}
		case 405:
		{
			mph = 120;
			upvyz = 0.25;
			maxscor = 85;
		}
	}
	if(speeds > mph) return 1;
	if(speeds >= maxscor)
	{
		if(speeds > 0)
		{
			//multiple = (mph / (speeds * 100));
			return SetVehicleVelocity(vehicleid, Vx+upvyz, Vy+upvyz, Vz+upvyz);
		}
	}
/*	if(speeds > 0)
	{
		multiple = (mph / (speeds * 100));
		return SetVehicleVelocity(vehicleid, Vx*multiple, Vy*multiple, Vz*multiple);
	}*/
	return 0;
}
