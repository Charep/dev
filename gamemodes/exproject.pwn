stock ResetWeapon(playerid, delgun)
{
	new id[13];
	new ammo[13];

    for(new h = 0; h < 13; h++)
	{
		GetPlayerWeaponData(playerid,h,id[h],ammo[h]);
	}

	pInfo[playerid][pWeapon][delgun] = 0;
	pInfo[playerid][pAmmo][delgun] = 0;
	ac_ResetPlayerWeapons(playerid);

	for(new h = 0; h < 13; h++)
	{
	    if( id[h] == delgun ) continue;
		ac_GivePlayerWeapon(playerid,id[h],0);
		id[h]=EOS; ammo[h]=EOS;
	}

	return 1;
}

stock SaveWeapon(playerid)
{
	new id[13],
    	ammo[13];

	for(new h = 0; h < 13; h++)
	{
		GetPlayerWeaponData(playerid,h,id[h],ammo[h]);
	}

	mysql_format:str_cmd("UPDATE `" #DB_ACCOUNTS "` SET `pWeapon` = '%d|%d|%d|%d|%d|%d|%d|%d|%d|%d' WHERE `ID` = '%d'",id[0],id[1],id[2],id[3],id[4],id[5],id[6],id[7],id[8],id[9],pInfo[playerid][pID]);
	mysql_function_query(mysql_connect_ID,str_cmd, false, "", "");

	mysql_format:str_cmd("UPDATE `" #DB_ACCOUNTS "` SET `pAmmo` = '%d|%d|%d|%d|%d|%d|%d|%d|%d|%d' WHERE `ID` = '%d'",ammo[0],ammo[1],ammo[2],ammo[3],ammo[4],ammo[5],ammo[6],ammo[7],ammo[8],ammo[9],pInfo[playerid][pID]);
	mysql_function_query(mysql_connect_ID,str_cmd, false, "", "");

	return ResetPlayerWeapons(playerid);
}

ac_GivePlayerWeapon(playerid, weaponid, ammo)
{
	pInfo[playerid][pWeapon][weaponid] += ammo;
 	GivePlayerWeapon(playerid,weaponid,ammo);
 	return 1;
}

ac_ResetPlayerWeapons(playerid)
{
	ResetPlayerWeapons(playerid);
 	for(new i=0;i<47;i++)
 	{
  		if(pInfo[playerid][pWeapon][i] > 0) GivePlayerWeapon(playerid,i,pInfo[playerid][pWeapon][i]);
   	}
   	return 1;
}

ac_ResetPlayerWeaponsf(playerid)
{
	ResetPlayerWeapons(playerid);
 	for(new i=0;i<47;i++)
 	{
  		pInfo[playerid][pWeapon][i] = 0;
  		if( i <= 15 ) pInfo[playerid][pAmmo][i] = 0;
   	}
   	return 1;
}

ac_Kick(playerid, typecheat)
{
	new bool:admins = false;

	foreach(Player, i) if(pInfo[i][pAdmin] != 0)
	{
		admins = true;
		break;
	}

	if(admins == false)
	{
		new strcheat[900];
	    format(strcheat,sizeof(strcheat),"[WARNING] %s был отключен от сервера за использования читов!",pInfo[playerid][pName]);
        SAM(COLOR_LIGHT_RED, strcheat );
		SCM(playerid,-1,"" #C_ORANGE "Вы были отсоединены от сервера в подозрении на читерство");
 		Kick(playerid);
	}
	else
	{
		switch(typecheat)
		{
			case 0: format:str_small("[WARNING]%s[%d] кикнут за использование чита на патроны или оружие!",pInfo[playerid][pName], playerid), SCM(playerid,-1,"" #C_ORANGE "Вы были отсоединены от сервера в подозрении на читерство"), Kick(playerid);
