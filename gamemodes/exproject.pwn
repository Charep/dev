forward Float:DistanceFromPointToPoint(Float: x1, Float: y1, Float: z1, Float: x2, Float: y2, Float: z2);
stock Float:DistanceFromPointToPoint(Float: x1, Float: y1, Float: z1, Float: x2, Float: y2, Float: z2)
{
        x1 = x1 - x2; 
        x1 = x1*x1; 
        y1 = y1 - y2;
        y1 = y1*y1;
        z1 = z1 - z2;
        z1 = z1*z1;
        x1 += y1 + z1;
        x1 = floatsqroot(x1);
        return x1;
}

forward Float:GetDistanceBetweenPlayers(player_x, player_y);
stock Float:GetDistanceBetweenPlayers(player_x, player_y)
{
        new Float:x1, Float:y1, Float:z1, Float:x2, Float:y2, Float:z2;
        GetPlayerPos(player_x, x1, y1, z1);
        GetPlayerPos(player_y, x2, y2, z2);
        new Float:distance = DistanceFromPointToPoint(x1, y1, z1, x2, y2, z2);
        return distance;
}

stock SetPlayerLook(playerid, Float:x, Float:y)
{
        new Float:pos_x;
        new Float:pos_y;
        new Float:angle;
        GetPlayerPos(playerid, pos_x, pos_y, angle);
        angle = atan2(y-pos_y, x-pos_x)-90.0;
        SetPlayerFacingAngle(playerid, angle);
        return 1;
}

