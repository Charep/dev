			mysql_function_query(mysql_connect_ID, str_small, false, "", "");

		    break;
		}
}
	if(params[0] == 101)
	{
		if(pInfo[playerid][pSmsDuty] != 1)
			{
				return SCM(playerid, 0xF48D4DFF, "Вы не вызывали МЧС!");
			}
		format:str_small("Ваше сообщение для 101:{ffffff} %s {F48D4D}[Для продолжения общения - /sms 101]", params[1]);
		SCM( playerid, 0xF48D4DFF, str_small);
		pInfo[playerid][pSmsDuty] = 1;
		foreach(Player, id)
		{
			if(pInfo[id][pFrak][0] == 5 && GetPVarInt(id,"duty") == 1)
				{
					format:str_small("Дежурный 101 - сообщение от %s[%d]: %s", pInfo[playerid][pName],playerid, params[1]);
					SCM( id, 0xF48D4DFF, str_small );
				}
		}
		return 1;
	}
	if(params[0] == 102)
	{
		if(pInfo[playerid][pSmsDuty] != 2)
			{
				return SCM(playerid, 0xF48D4DFF, "Вы не вызывали полицию!");
			}
		format:str_small("Ваше сообщение для 102:{ffffff} %s {F48D4D}[Для продолжения общения - /sms 102]", params[1]);
		SCM( playerid, 0xF48D4DFF, str_small);
		pInfo[playerid][pSmsDuty] = 2;
		foreach(Player, id)
		{
			if(pInfo[id][pFrak][0] == 1 || pInfo[id][pFrak][0] == 2 || pInfo[id][pFrak][0] == 3 && GetPVarInt(id,"duty") == 1)
				{
					format:str_small("Дежурный 102 - сообщение от %s[%d]: %s", pInfo[playerid][pName],playerid, params[1]);
					SCM( id, 0xF48D4DFF, str_small );
				}
		}
		return 1;
	}
	if(params[0] == 103)
	{
		if(pInfo[playerid][pSmsDuty] != 3)
			{
				return SCM(playerid, 0xF48D4DFF, "Вы не вызывали скорую помощь!");
			}
		format:str_small("Ваше сообщение для 103:{ffffff} %s {F48D4D}[Для продолжения общения - /sms 103]", params[1]);
		SCM( playerid, 0xF48D4DFF, str_small);
		pInfo[playerid][pSmsDuty] = 3;
		foreach(Player, id)
