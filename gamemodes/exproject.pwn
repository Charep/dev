	foreach(Player, i)
	{
		if(pInfo[i][pFrak][0]==1 || pInfo[i][pFrak][0]==2 || pInfo[i][pFrak][0]==3 || pInfo[i][pFrak][0]==4 || pInfo[i][pFrak][0]==5 || pInfo[i][pFrak][0]==6 || pInfo[i][pFrak][0]==8)
