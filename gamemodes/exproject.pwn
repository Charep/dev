			case 2: format:str_small("[WARNING] %s[%d] заподозрен в использовании Speed-Hack!",pInfo[playerid][pName], playerid);
			case 3: format:str_small("[WARNING] %s[%d] заподозрен в использовании Speed-Hack!",pInfo[playerid][pName], playerid);
			default: format:str_small("[WARNING] %s[%d] обнаружен неизвестный код чита!",pInfo[playerid][pName],playerid);
		}
		SAM(COLOR_LIGHT_RED, str_small);

	}
