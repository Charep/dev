CMD:id(playerid, params[])
{
    if(sscanf(params, "s[24]", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /id [Nick_Name]");

    if( strlen( params[0] ) > 24 ) return SCM(playerid, -1, "Слишком длинный ник");

    new num;

	foreach(Player, i)
	{
	   	if(strfind(pInfo[i][pName], params[0], true) !=-1)
	   	{
            if(!strcmp(params[0],pInfo[i][pName],true))
            {
                pformat:( ""C_GRAY"%s {ffffff}[%d]", pInfo[i][pName], i);
				psend:( playerid, COLOR_WHITE );
				num++;
                break;
            }
	   	    if(i == 0) SCM(playerid,-1,"Все найденные совпадения:");
		   	if(num >= 5) break;
			num++;
		   	pformat:( ""C_GRAY"%s {ffffff}[%d]", pInfo[i][pName], i);
			psend:( playerid, COLOR_WHITE );
	   	}
	}
	if(num == 0) return SCM(playerid,-1,"Совпадений не найдено!");

	return 1;
}

