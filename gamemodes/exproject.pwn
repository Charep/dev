CMD:talk(playerid, params[])
{
    if(pInfo[playerid][pFrak][0]!=7) return SCM(playerid,-1,"Вы не состоите в фракции ТРК");
    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /talk [ид игрока]");
    if(GetPVarInt(playerid,"gLogged")!=1) return SCM(playerid,-1,"Игрок с данным ид не найден!");
    if(params[0]==playerid) return SCM(playerid,-1,"Вы не можете позвать в эфир самого себя!");

   	pformat:("Вы пригласили в эфир "C_GRAY"%s"C_WHITE"",pInfo[params[0]][pName]);
	psend:( playerid, COLOR_WHITE );

	format:str_cmd("{ffffff}Сотрудник ТРК "C_GRAY"%s"C_WHITE" приглашает вас принять участие в эфире",pInfo[playerid][pName]);
	SPD(params[0],dFrak+12,info,"" #C_GRAY "Приглашение",str_cmd,"Принять","Закрыть");

	SetPVarInt(params[0],"talkid",playerid);

	return 1;
}

CMD:talkoff(playerid, params[])
{
    if(pInfo[playerid][pFrak][0]!=7) return SCM(playerid,-1,"Вы не состоите в фракции ТРК");
    if(sscanf(params, "d", params[0])) return SCM(playerid, COLOR_WHITE, "Используйте: /talkoff [ид игрока]");
    if(GetPVarInt(playerid,"gLogged")!=1) return SCM(playerid,-1,"Игрок с данным ид не найден!");
    if(params[0]==playerid) return SCM(playerid,-1,"Вы не можете прервать эфир самому себе!");
    if(GetPVarInt(params[0],"talk")!=1) return SCM(playerid,-1,"Данный человек не находиться в эфире!");

	pformat:("Сотрудник ТРК "C_GRAY"%s"C_WHITE" отключил вас от эфира",pInfo[playerid][pName]);
	psend:( params[0], COLOR_WHITE );

	DeletePVar(params[0],"talk");

	pformat:("Сотрудник ТРК "C_GRAY"%s"C_WHITE" отключил "C_GRAY"%s"C_WHITE" от эфира!",pInfo[playerid][pName],pInfo[params[0]][pName]);

	foreach(Player, i)
	{
		if(pInfo[i][pFrak][0]!=7) continue;
		psend:( i, COLOR_WHITE );
	}

	return 1;
}

CMD:p(playerid)
{
	if( c_info[playerid][cStatus] != 2) return SCM(playerid,-1,"Вам никто не звонил!");

	new id = c_info[playerid][cCaller];

	if( c_info[id][cStatus] != 1) return SCM(playerid,-1,"Вам никто не звонил!");

	pformat:( "Абонент %d ответил на ваш звонок",pInfo[playerid][pNumber]);
	psend:( id, COLOR_WHITE );

	if( GetPVarInt( id, "payphone") == 0 )
	{
		pformat:( "Вы ответили на вызов от абонента %d",pInfo[id][pNumber]);
		psend:( playerid, COLOR_WHITE );
	}
	else
	{
		pformat:( "Вы ответили на вызов от таксофона №%d",GetPVarInt( id, "payphone"));
		psend:( playerid, COLOR_WHITE );
	}

	PlayAudioStreamForPlayer(id,"http://exrptest.myarena.ru/vzal.mp3");

	c_info[id][cStatus] = 3;
	c_info[playerid][cStatus] = 4;

	DeletePVar(id,"goodok");

    return 1;
}

CMD:c(playerid)
{
	if( c_info[playerid][cStatus] == 2) // сбросить входящий вызов
	{
	    new id = c_info[playerid][cCaller];

		SCM(playerid,-1,"Вы сбросили входящий вызов");
		SCM(id,-1,"Абонент сбросил Ваш вызов");

		c_info[playerid][cStatus] = 0;
		c_info[playerid][cCaller] = 0;
		c_info[playerid][cStatus] = 0;

		c_info[id][cStatus] = 0;
		c_info[id][cCaller] = 0;
		c_info[id][cStatus] = 0;

		return 1;
	}

	if( c_info[playerid][cStatus] == 1) // сбросить исходящий вызов
	{
	    new id = c_info[playerid][cAnswerer];

		SCM(playerid,-1,"Вы сбросили исходящий вызов");
		SCM(id,-1,"Абонент сбросил входящий вызов");

		c_info[playerid][cStatus] = 0;
		c_info[playerid][cCaller] = 0;
		c_info[playerid][cStatus] = 0;

		c_info[id][cStatus] = 0;
		c_info[id][cCaller] = 0;
		c_info[id][cStatus] = 0;

		return 1;
	}

	if( c_info[playerid][cStatus] == 3) // сбросить разговор от звонящего
	{
	    new id = c_info[playerid][cAnswerer];

		SCM(playerid,-1,"Вы завершили вызов");
		SCM(id,-1,"Абонент сбросил вызов");

		c_info[playerid][cStatus] = 0;
		c_info[playerid][cCaller] = 0;
		c_info[playerid][cStatus] = 0;

		if( GetPVarInt( playerid, "payphone") == 0 )
		{
			pInfo[playerid][pPhonemoney] -= GetPVarInt( playerid, "calltime" ) * 2;
	 		mysql_format:str_small("UPDATE `" #DB_ACCOUNTS "` SET `Phonemoney` = '%d'  WHERE `ID` = '%d'", pInfo[playerid][pPhonemoney], pInfo[playerid][pID]);
			mysql_function_query(mysql_connect_ID, str_small, false, "", "");

			pformat:( "Длительность звонка: " #C_GRAY "%s - {ffffff}со счета мобильного снятно: " #C_GRAY "%d",Convert(GetPVarInt(playerid,"calltime")), GetPVarInt( playerid, "calltime" ) * 2 );
			psend:( playerid, COLOR_WHITE );
		}

		DeletePVar(playerid,"calltime");
		if( GetPVarInt( playerid, "payphone" ) != 0 )
		{
			DeletePVar( playerid, "payphone" );
			TogglePlayerControllable( playerid, true );
		}

		c_info[id][cStatus] = 0;
		c_info[id][cCaller] = 0;
		c_info[id][cStatus] = 0;

		return 1;
	}

	if( c_info[playerid][cStatus] == 4) // сбросить разговор от отвечающего
	{
	    new id = c_info[playerid][cCaller];

		SCM(id,-1,"Вы завершили вызов");
		SCM(playerid,-1,"Абонент сбросил вызов");

		c_info[id][cStatus] = 0;
		c_info[id][cCaller] = 0;
		c_info[id][cStatus] = 0;

        if( GetPVarInt( id, "payphone") == 0 )
		{
			pInfo[id][pPhonemoney] -= GetPVarInt( id, "calltime" ) * 2;
	 		mysql_format:str_small("UPDATE `" #DB_ACCOUNTS "` SET `Phonemoney` = '%d'  WHERE `ID` = '%d'", pInfo[id][pPhonemoney], pInfo[id][pID]);
			mysql_function_query(mysql_connect_ID, str_small, false, "", "");

			pformat:( "Длительность звонка: " #C_GRAY "%s - {ffffff}со счета мобильного снятно: " #C_GRAY "%d",Convert(GetPVarInt(id,"calltime")), GetPVarInt( id, "calltime" ) * 2 );
			psend:( id, COLOR_WHITE );
		}

		DeletePVar(id,"calltime");
		if( GetPVarInt( id, "payphone" ) != 0 )
		{
			DeletePVar( id, "payphone" );
			TogglePlayerControllable( id, true );
		}

		c_info[playerid][cStatus] = 0;
		c_info[playerid][cCaller] = 0;
		c_info[playerid][cStatus] = 0;

		return 1;
	}

	SCM(playerid,-1,"У Вас нет входящих/исходящих вызовов!");

	return 1;
}

CMD:moto(playerid)
{
	if(GetPVarInt(playerid,"motoid")==0) return SCM(playerid,-1,"Вы не арендовали мопед!");
	new Float:p1,Float:p2,Float:p3; GetVehiclePos(GetPVarInt(playerid,"motoid"),p1,p2,p3);
