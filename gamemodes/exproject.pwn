				for( new z; z != 10; z++ )
				{
					if(_invPlayer[playerid][invSlot][z] == 375 || _invPlayer[playerid][invSlot][z] == 376) count++;
				}

				if( count >= 20 ) return SCM(playerid, -1, "Вы не можете переносить с собой больше, чем 20 канистр!");

				if( params[1] == 1 )
				{
				    if( params[0] * 150 > pInfo[playerid][pMoney] ) return SCM( playerid, -1, "Вам не хватает денег на покупку канистр!");

				    if(IsSpace(playerid,375)) return SCM(playerid, -1,"У вас не хватает места в инвентаре!");

				    GivePlayerItem(playerid,375,params[0]);

					GiveMoney(playerid,-params[0] * 150);

					SendMes(playerid, -1,"Вы успешно закупили %d канистр 10L на %dр", params[0], params[0]*150);

				    return 1;
				}

		        if( params[1] == 2 )
				{
				    if( params[0] * 250 > pInfo[playerid][pMoney] ) return SCM( playerid, -1, "Вам не хватает денег на покупку канистр!");

				    if(IsSpace(playerid,376)) return SCM(playerid, -1,"У вас не хватает места в инвентаре!");

				    GivePlayerItem(playerid,376,params[0]);

				    GiveMoney(playerid,-params[0] * 250);

					SendMes(playerid, -1,"Вы успешно закупили %d канистр 20L на %dр", params[0], params[0]*250);

				    return 1;
				}

		        found = true;
		        break;
		    }
		}

		if( found == false ) return SCM( playerid, -1, "Вы должны находиться на заправке!");
	}
	else return SCM( playerid, -1, "Вы не состоите в организации или не являетесь ее лидером!");

	return 1;
}
CMD:tow(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,"Вы должны находиться в эвакуаторе");

    if(pInfo[playerid][pFrak][0] != 0 || pInfo[playerid][pOrg][0] > 0)
    {
    new car = GetPlayerVehicleID(playerid);

    if(pInfo[playerid][pFrak] == 3 && car >= gibdd[0] && car <= gibdd[1] && GetPlayerVehicleSeat(playerid)==0)
	{
		//if(pInfo[playerid][pFrak]!=3) return RemovePlayerFromVehicle(playerid), SCM(playerid,-1,"У вас нет ключа от этого автомобиля!");
		if(GetVehicleModel(car)!=525) return SCM(playerid,-1,"Чтобы прицепить транспорт, Вы должны находиться в эвакуаторе!");
	}

	if( pInfo[playerid][pOrg][0] > 0 )
	{
 		if( oInfo[ pInfo[playerid][pOrg][0] ][oType] != 1 || GetVehicleModel(car) != 525 ) return SCM(playerid,-1,"Вы должны работать в СТО и находиться в эвакуаторе");
 	}

 	if(GetVehicleModel(car)!=525) return 1;

    if(pInfo[playerid][pFrak][0] == 2 ||pInfo[playerid][pFrak][0] == 3 || oInfo[ pInfo[playerid][pOrg][0] ][oType] == 1)
	{
	    //new Float:x, Float:y, Float:z;
		//GetBootVehiclePos(car,x,y,z);

		if(IsTrailerAttachedToVehicle(car))
		{
		    DetachTrailerFromVehicle(car);
		    SCM(playerid,-1,"Вы отцепили транспорт от эвакуатора!");
		    return 1;
		}

		new Float:posx, Float:posy, Float:posz, bool:found = false;
  		new Float:oldposx, Float:oldposy, Float:oldposz;
  		new Float:tempposx, Float:tempposy, Float:tempposz;
  		new Float:Radi=6.0;        //Расстояние от тачки
  		new Float:AngRange=90.0;    //максимальный радиус

  		new Float:plAngle;    GetVehicleZAngle(car, plAngle);
    	new Float:crAngle,Float:Angle,Float:Angle2;

		for(new i = 0; i < MAX_VEHICLES; i++)
  		{
		    if(i == car) continue;

			GetVehiclePos(car, posx, posy, posz);
			GetVehiclePos(i, oldposx, oldposy, oldposz);
			tempposx = (oldposx -posx);
			tempposy = (oldposy -posy);
			tempposz = (oldposz -posz);
			if (((tempposx < Radi) && (tempposx > -Radi)) && ((tempposy < Radi) && (tempposy > -Radi)) && ((tempposz < Radi) && (tempposz > -Radi)))
			{
				GetVehicleZAngle(i, crAngle);
				Angle = (plAngle -crAngle);
				Angle2= (Angle-180.0);
				if (( ((Angle < AngRange) && (Angle > -AngRange)) || ((Angle2 < AngRange) && (Angle2 > -AngRange)) ))
				{
					AttachTrailerToVehicle(i, car);
					SCM(playerid,-1,"Вы прицепили транспорт к эвакуатору");
					if( pInfo[playerid][pOrg][0] <= 0 ) SCM(playerid,-1,"Отправляйтесь на " #C_GRAY "Ш/С {ffffff}для ареста авто " #C_GRAY "(/gps > Прочее > 7)");
					SCM(playerid,-1,"Чтобы отцепить транспорт от эвакуатора, введите " #C_GRAY "/tow {ffffff}еще раз!");
					found = true;
					break;
				}
			}
		}
		if(found == false) SCM(playerid, -1, "Поблизости транспорт - не найден !");
	}
	else return SCM(playerid,-1,"Вы не состоите ни в одной из фра666кций!");
