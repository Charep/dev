CMD:pm(playerid, params[])
{
    if(GetPVarInt(playerid,"mute")!=0) return SendMes(playerid,-1,"" #C_GRAY "[MUTE] У Вас отключен OOC чат на: " #C_ORANGE "%d секунд",GetPVarInt(playerid,"mute"));
	if(sscanf(params, "ds[150]", params[0],params[1])) return SCM(playerid, COLOR_WHITE, "Используйте: /pm [id] [текст]");
	if(params[0]==playerid) return SCM(playerid,-1,"Вы не можете писать самому себе!");
	if(!IsPlayerConnected(params[0])) return SCM(playerid,-1,"Игрока с таким id нет онлайн");
	if(GetPVarInt(params[0],"pmoff")==1) return SCM(playerid,-1,"Игрок выключил свое PM");

	pformat:( "{E7F56D}(( PM | %s[%d]: %s ))",pInfo[playerid][pName],playerid,params[1]);
	psend:( params[0], COLOR_WHITE );

	pformat:( "{E7F56D}(( PM | для %s[%d]: %s ))",pInfo[params[0]][pName],params[0],params[1]);
	psend:( playerid, COLOR_WHITE );

	format:str_small("" #C_LIGHTBLUE "[PM] От %s[%d] для %s[%d]: %s",pInfo[playerid][pName],playerid,pInfo[params[0]][pName],params[0],params[1]);
